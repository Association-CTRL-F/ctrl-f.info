{
	"name": "ctrl-f.info",
	"private": true,
	"version": "1.0.0-6",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"format": "prettier --write .",
		"release": "release-it",
		"postinstall": "pnpm run format",
		"postuninstall": "pnpm run format"
	},
	"imports": {
		"#assets/*": "./src/assets/*",
		"#components/*": "./src/components/*.tsx",
		"#data/*": "./src/data/*.ts",
		"#types/*": "./src/types/*.ts",
		"#hooks/*": "./src/hooks/*.tsx",
		"#lib/*": "./src/lib/*.ts"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.6",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"babel-plugin-react-compiler": "^1.0.0",
		"eslint": "^9.39.1",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.24",
		"globals": "^16.5.0",
		"prettier": "^3.6.2",
		"release-it": "^19.0.6",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.47.0",
		"unocss": "^66.5.6",
		"vite": "npm:rolldown-vite@7.2.5"
	},
	"pnpm": {
		"overrides": {
			"vite": "npm:rolldown-vite@7.2.5"
		}
	},
	"prettier": {
		"trailingComma": "es5",
		"semi": true,
		"singleQuote": true,
		"useTabs": true,
		"quoteProps": "as-needed",
		"bracketSpacing": true,
		"arrowParens": "always",
		"printWidth": 80
	},
	"volta": {
		"node": "24.11.0"
	}
}
