{
	"name": "ctrl-f.info",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"format": "prettier --write .",
		"postinstall": "pnpm run format",
		"postuninstall": "pnpm run format"
	},
	"imports": {
		"#assets/*": "./src/assets/*",
		"#components/*": "./src/components/*.tsx",
		"#data/*": "./src/data/*.ts",
		"#types/*": "./src/types/*.ts"
	},
	"dependencies": {
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@types/node": "^24.10.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@vitejs/plugin-react": "^5.1.0",
		"babel-plugin-react-compiler": "^19.1.0-rc.3",
		"eslint": "^9.39.1",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.24",
		"globals": "^16.5.0",
		"prettier": "^3.6.2",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.46.4",
		"unocss": "^66.5.5",
		"vite": "npm:rolldown-vite@7.2.2"
	},
	"pnpm": {
		"overrides": {
			"vite": "npm:rolldown-vite@7.2.2"
		}
	},
	"prettier": {
		"trailingComma": "es5",
		"semi": true,
		"singleQuote": true,
		"useTabs": true,
		"quoteProps": "as-needed",
		"bracketSpacing": true,
		"arrowParens": "always",
		"printWidth": 80
	},
	"volta": {
		"node": "24.11.0"
	}
}
